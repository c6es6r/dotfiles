#workspaces,
#window,
#wireplumber,
#pulseaudio,
#cpu,
#memory,
#temperature,
#network,
#clock,
#tray {
    margin: 5 10px;
    padding: 0 10px;
    border-radius: 5;
}

#wireplumber,
#pulseaudio {
    background-color: #cc241d;
}

#cpu {
    background-color: #98971a;
}

#memory {
    background-color: #d79921;
}

#network {
    background-color: #458588;
}

#clock {
    background-color: #b16286;
}

