* {
    font-family: JetBrainsMonoNerdFont-Regular, FontAwesome, Roboto, Helvetica, Arial, sans-serif;
    font-size: 14px;
    color: #fbf1c7;
}

window#waybar {
    background-color: transparent; /*#282828;*/
}

#workspaces, #window {

    background-color: #689d6a;
}

